datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Plugin {
  id                String         @id @default(auto()) @map("_id") @db.ObjectId
  pluginId          String         @unique
  name              String?
  author            String?
  description       String?
  repo              String
  createdAt         Int
  nextUpdateAt      Int?
  lastCommitAt      Int?
  stargazers        Int?
  subscribers       Int?
  latestRelease     String?
  latestReleaseDesc String?
  latestReleaseAt   Int?
  totalDownloads    Int?
  zScoreTrending    Int?
  aiDescription     String?
}

model ReleaseDownloads {
  id              String         @id @default(auto()) @map("_id") @db.ObjectId
  pluginId        String         @unique
  timestamp       Int
  deltaDownload   Int
}

model PluginTags {
  id               String         @id @default(auto()) @map("_id") @db.ObjectId
  tag              String
  pluginId         String
}
